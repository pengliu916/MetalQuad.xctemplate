            </dict>
            <key>Release</key>
            <dict>
                <key>MTL_ENABLE_DEBUG_INFO</key>
                <string>NO</string>
            </dict>
        </dict>
    </dict>
	<key>Targets</key>
	<array>
		<dict>
			<key>SharedSettings</key>
			<dict>
				<key>SWIFT_OBJC_BRIDGING_HEADER</key>
